@model IEnumerable<Quiver.Models.ClassificacaoViewModels>

<div id="tabela" tabindex="0" style="overflow: hidden; outline: none;">
    <div class="table-responsive" tabindex="0" style="overflow: hidden; outline: none;">
        <table id="data-table-command" class="table table-striped table-vmiddle" aria-busy="false">
            <thead>
                <tr>
                    <th data-column-id="id" data-type="numeric" data-visible="false">ID</th>
                    <th data-column-id="descricao">Nome da Classificação</th>
                    <th data-column-id="inicioIntervalo">De</th>
                    <th data-column-id="fimIntervalo">Até</th>
                </tr>
            </thead>
            <tbody>
                @for(int i = 0; i<@Model.quantidade; i++)
                {
                    <tr>
                        <td>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="zmdi zmdi-account"></i></span>
                                <div class="fg-line">
                                    @Html.TextBoxFor(m => m.Descricao)
                                </div>
                            </div>
                        </td>
                        <td>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="zmdi zmdi-account"></i></span>
                                <div class="fg-line">
                                    @Html.TextBoxFor(m => m.InicioIntervalo)
                                </div>
                            </div>
                        </td>
                        <td>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="zmdi zmdi-account"></i></span>
                                <div class="fg-line">
                                    @Html.TextBoxFor(m => m.FimIntervalo)
                                </div>
                            </div>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>




