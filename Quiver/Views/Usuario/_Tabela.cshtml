@model IList<Quiver.Models.UsuarioRow>

<div id="tabela" tabindex="0" style="overflow: hidden; outline: none;">
    <div class="table-responsive" tabindex="0" style="overflow: hidden; outline: none;">
        <table id="data-table-command" class="table table-striped table-vmiddle" aria-busy="false">
            <thead>
                <tr>
                    <th data-column-id="id" data-visible="false">ID</th>
                    <th data-column-id="login">Login</th>
                    <th data-column-id="nome">Nome</th>
                    <th data-column-id="perfil">Perfil</th>
                    <th data-column-id="Logado" data-visible="false">Logado</th>
                    <th data-column-id="commands" data-formatter="commands" data-sortable="false">Opções</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var row in @Model)
                {
                    <tr>
                        <td>@row.Id</td>
                        <td>@row.Login</td>
                        <td>@row.Nome</td>
                        <td>@row.Perfil</td>    
                        <td>@row.Logado</td>                                        
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

@section scripts
{    
    <script type="text/javascript" src="~/Scripts/Custom/usuario/usuarios.js"></script>
    <script type="text/javascript" src="~/Scripts/Custom/usuario/listarUsuarios.js"></script>
}


