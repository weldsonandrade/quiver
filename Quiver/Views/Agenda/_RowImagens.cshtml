@model List<string> 

@if (Model.Count > 0)
{
   <!-- GALERIA DE FOTOS -->
    <div class="col-md-12  p-t-10">

        <small class=" p-b-5">Galeria de imagens</small>
        <section id="gallery" class="p-t-10">
            <div class="lightbox m-l-0" style="max-width: 100%; max-height:100%">
                @foreach (var foto in Model)
                {
                    var imagem = String.Format("data:image/jpeg;base64,{0}", foto);
                    <div data-src="@imagem" class="col-lg-1 col-sm-4 col-xs-6 p-l-0 p-r-5 p-b-5" data-sub-html="">
                        <div class="lightbox-item">
                            <img src="@imagem" alt="">
                        </div>
                    </div>
                }
            </div>
        </section>
    </div>
    <!-- FIM GALERIA DE FOTOS -->
}
